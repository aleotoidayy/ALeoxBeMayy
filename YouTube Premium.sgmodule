#!name= YouTube Premium
#!desc= 𝗔𝗹𝗲𝗼𝗼𝘅𝗠𝗮𝘆𝘆
#!author= 𝗠𝗮𝘆𝗠𝗮𝘆𝘅𝗔𝗟𝗲𝗼𝗼
#!icon= https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/YouTube.png

[Rule]
AND,((DOMAIN-SUFFIX,googlevideo.com), (PROTOCOL,UDP)),REJECT
AND,((DOMAIN,youtubei.googleapis.com), (PROTOCOL,UDP)),REJECT

[URL Rewrite]
^https?:\/\/[\w-]+\.googlevideo\.com\/(?!(dclk_video_ads|videoplayback\?)).+&oad _ reject-200
^https?:\/\/(www|s)\.youtube\.com\/api\/stats\/ads _ reject-200

[Script]
youtube.response = type=http-response,pattern=^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/(browse|next|player|search),requires-body=1,max-size=-1,binary-body-mode=1,script-path=https://raw.githubusercontent.com/aleotoidayy/ALeoxBeMayy/main/YouTube.js

[MITM]
hostname = %APPEND% *.googlevideo.com,youtubei.googleapis.com,www.youtube.com