[Script]
SoundCloudGo+=type=http-response,pattern=https://api-mobile.soundcloud.com/configuration/ios,requires-body=1,script-path=https://raw.githubusercontent.com/aleotoidayy/ALeoxBeMayy/main/soundcloud.js

youtube.response = type=http-response,pattern=^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/(browse|next|player|search|reel\/reel_watch_sequence|guide|account\/get_setting|get_watch),requires-body=1,max-size=-1,binary-body-mode=1,engine=default,script-path=https://raw.githubusercontent.com/aleotoidayy/ALeoxBeMayy/main/YouTube.js,argument={"lyricLang":"en","captionLang":"en","blockUpload":false,"blockImmersive":false,"debug":false}

[MITM]
hostname = %APPEND% api-mobile.soundcloud.com, youtubei.googleapis.com